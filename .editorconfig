# https://editorconfig.org/ - Docs

# top-most EditorConfig file
root = true

# Indentation in all files
[*]
indent_size = 4

# editorconfig.org
root = true

[*.{js,jsx,ts,tsx,vue}]
indent_style = space
indent_size = 4
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true
max_line_length = 200
